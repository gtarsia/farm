config_version=4

_global_script_classes=[ {
"base": "RigidBody2D",
"class": "Chicken",
"language": "GDScript",
"path": "res://scenes/chickens/Chicken.gd"
}, {
"base": "Node",
"class": "ChickenSelector",
"language": "GDScript",
"path": "res://scenes/ChickenSelector.gd"
} ]
_global_script_class_icons={
"Chicken": "",
